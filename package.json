    "bcrypt-nodejs": "^0.0.3",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2"
  }
}
